{
  "name": "scope-reporter",
  "version": "0.0.0-semantic-release",
  "main": "dist/scope-reporter.js",
  "module": "src/scope-reporter.js",
  "description": "traks scope identifiers during processing",
  "keywords": [
    "scope",
    "tracking"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "nyc --temp-directory build/nyc ava",
    "test": "ava",
    "docs": "documentation readme src/scope-reporter.js --section=API",
    "semantic-release": "semantic-release",
    "pretest": "rollup -c tests/rollup.config.js",
    "posttest": "npm run prepare && markdown-doctest",
    "precover": "rollup -c tests/rollup.config.js",
    "prepare": "rollup -c",
    "lint": "documentation lint src/scope-reporter.js"
  },
  "dependencies": {
    "expression-expander": "6.0.1"
  },
  "devDependencies": {
    "cracks": "^3.1.2",
    "semantic-release": "^11.1.0",
    "markdown-doctest": "^0.9.1",
    "rollup": "^0.53.1",
    "rollup-plugin-multi-entry": "^2.0.2",
    "nyc": "^11.4.1",
    "ava": "^0.24.0",
    "xo": "^0.19.0",
    "documentation": "^5.3.5"
  },
  "engines": {
    "node": ">=8.9.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arlac77/scope-reporter.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/scope-reporter/issues"
  },
  "homepage": "https://github.com/arlac77/scope-reporter#readme",
  "release": {
    "verifyRelease": "cracks"
  },
  "ava": {
    "files": [
      "build/*-test.js"
    ],
    "presets": [
      null
    ],
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "include": [
      "build/*-test.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./build/coverage"
  },
  "xo": {
    "space": true
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
