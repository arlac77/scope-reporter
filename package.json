{
  "name": "scope-reporter",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./src/scope-reporter.mjs",
  "exports": {
    ".": "./src/scope-reporter.mjs"
  },
  "description": "traks scope identifiers during processing",
  "keywords": [
    "scope",
    "tracking"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "test": "npm run test:ava",
    "test:ava": "ava --timeout 2m tests/*.mjs",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme src/scope-reporter.mjs --section=API && documentation readme ./src/scope-reporter.mjs --section=API",
    "lint": "npm run lint:docs",
    "lint:docs": "documentation lint src/scope-reporter.mjs && documentation lint ./src/scope-reporter.mjs"
  },
  "dependencies": {
    "expression-expander": "7.0.6"
  },
  "devDependencies": {
    "ava": "^3.13.0",
    "c8": "^7.3.5",
    "documentation": "^13.1.0",
    "semantic-release": "^17.2.1"
  },
  "engines": {
    "node": ">=12.19.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/scope-reporter.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/scope-reporter/issues"
  },
  "homepage": "https://github.com/arlac77/scope-reporter#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-esm-only",
      "arlac77/template-github"
    ]
  }
}
