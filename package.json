{
  "name": "scope-reporter",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/scope-reporter.js",
  "module": "src/scope-reporter.js",
  "description": "traks scope identifiers during processing",
  "keywords": [
    "scope",
    "tracking"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "c8 --temp-directory build/coverage ava && c8 report -r lcov --temp-directory build/coverage",
    "docs": "documentation readme src/scope-reporter.js --section=API",
    "lint": "documentation lint src/scope-reporter.js",
    "posttest": "markdown-doctest && npm run prepare",
    "pretest": "rollup -c tests/rollup.config.js",
    "test": "ava"
  },
  "dependencies": {
    "expression-expander": "7.0.0"
  },
  "devDependencies": {
    "ava": "^1.0.1",
    "c8": "^3.2.1",
    "cracks": "^3.1.2",
    "documentation": "^9.1.1",
    "esm": "^3.0.84",
    "markdown-doctest": "^0.9.1",
    "semantic-release": "^15.13.2"
  },
  "engines": {
    "node": ">=10.15.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/scope-reporter.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/scope-reporter/issues"
  },
  "homepage": "https://github.com/arlac77/scope-reporter#readme",
  "release": {
    "verifyRelease": "cracks"
  },
  "ava": {
    "files": [
      "tests/*-test.js",
      "tests/*-test.mjs"
    ],
    "require": [
      "esm"
    ],
    "babel": false,
    "compileEnhancements": false,
    "extensions": [
      "js",
      "mjs"
    ]
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template-esm-only.git"
    }
  }
}
