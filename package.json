{
  "name": "scope-reporter",
  "description": "traks scope identifiers during processing",
  "main": "dist/ScopeReporter.js",
  "module": "src/ScopeReporter.js",
  "scripts": {
    "cover": "npm run prepublish && istanbul cover node_modules/.bin/_mocha -- -u exports tests/*_test.js",
    "test": "mocha tests/*_test.js",
    "docs": "jsdoc2md -l off -t doc/README.hbs -f src/*.js >README.md",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "prepublish": "rollup --output=dist/ScopeReporter.js -c -- src/ScopeReporter.js",
    "pretest": "npm run prepublish",
    "posttest": "markdown-doctest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/scope-reporter.git"
  },
  "engines": {
    "node": ">=6.9.3"
  },
  "keywords": [
    "scope",
    "tracking"
  ],
  "release": {
    "verifyRelease": "cracks"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "cracks": "^3.1.2",
    "cz-conventional-changelog": "^1.2.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "semantic-release": "^6.3.5",
    "jsdoc-to-markdown": "^2.0.1",
    "markdown-doctest": "^0.9.1",
    "rollup": "^0.41.1"
  },
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/arlac77/scope-reporter/issues"
  },
  "homepage": "https://github.com/arlac77/scope-reporter#readme",
  "dependencies": {
    "expression-expander": "5.3.6"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "version": "0.0.0-semantic-release",
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ]
}