{
  "name": "scope-reporter",
  "description": "traks scope identifiers during processing",
  "main": "dist/ScopeReporter.js",
  "module": "src/ScopeReporter.js",
  "scripts": {
    "cover": "npm run build && ./node_modules/istanbul/lib/cli.js cover --hook-run-in-context ./node_modules/mocha/bin/_mocha -- --R spec --U exports tests/*_test.js",
    "doc": "./node_modules/.bin/jsdoc lib/*.js",
    "test": "npm run build && mocha tests/*_test.js && markdown-doctest",
    "build": "./node_modules/rollup/bin/rollup --format=cjs --output=dist/ScopeReporter.js -- src/ScopeReporter.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "docs": "jsdoc2md -l off -t doc/README.hbs -f src/ScopeReporter.js > README.md"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/scope-reporter.git"
  },
  "engines": {
    "node": ">=6.9.1"
  },
  "keywords": [
    "scope",
    "tracking"
  ],
  "release": {
    "verifyRelease": "cracks"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "cracks": "^3.1.2",
    "cz-conventional-changelog": "^1.2.0",
    "istanbul": "^0.4.5",
    "jsdoc": "3.4.2",
    "mocha": "^3.1.2",
    "rollup": "^0.36.3",
    "semantic-release": "^6.3.2",
    "jsdoc-to-markdown": "^2.0.1",
    "markdown-doctest": "^0.9.0"
  },
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/arlac77/scope-reporter/issues"
  },
  "homepage": "https://github.com/arlac77/scope-reporter#readme",
  "dependencies": {
    "expression-expander": "^5.2.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "version": "0.0.0-semantic-release",
  "contributors": [{
    "name": "Markus Felten",
    "email": "markus.felten@gmx.de"
  }]
}
